<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <HediffDef>
    <hediffClass>HediffWithComps</hediffClass>
    <defaultLabelColor>(0.8, 0.8, 0.35)</defaultLabelColor>
    <defName>Electrocute</defName>
    <label>electrocuted</label>
    <initialSeverity>0.1</initialSeverity>
    <comps>
      <li Class="HediffCompProperties_Disappears">
        <compClass>HediffComp_Disappears</compClass>
        <disappearsAfterTicks>10000</disappearsAfterTicks>
      </li>
      <li Class="HediffCompProperties_SeverityPerDay">
        <compClass>HediffComp_SeverityPerDay</compClass>
        <severityPerDay>-4</severityPerDay>
      </li>
    </comps>
    <stages>
      <li>
        <minSeverity>0.1</minSeverity>
        <label>Painful</label>
        <painFactor>1.6</painFactor>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.3</offset>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.4</minSeverity>
        <label>Paralyzed</label>
        <painFactor>1.8</painFactor>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.3</setMax>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.6</minSeverity>
        <label>Shock</label>
        <painFactor>2.0</painFactor>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.1</setMax>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
</Defs>